{
    "name": "@dsquare-gbu/vue-starter",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "init": "npm run @run && npm run @assets",
        "serve": "vue-cli-service serve",
        "build": "npm run init && vue-cli-service build && npm run @build",
        "lint": "npm run init && vue-cli-service lint",
        "@run": "babel ./run/src --out-dir ./run/dist --config-file ./babel.run-config.js --delete-dir-on-start --minified --source-maps",
        "@assets": "npm run @setup && npm run @compile",
        "@setup": "node ./run/dist/setup.js",
        "@compile": "node ./run/dist/compile.js",
        "@build": "node ./run/dist/build.js",
        "@compose-pug": "node ./run/dist/compose.js --pug ./src/assets/compose/html ./src/assets/compose/pug",
        "@compose-vue": "node ./run/dist/compose.js --vue ./src/assets/compose/html ./src/assets/compose/pug ./src/assets/compose/vue ./src/views/templates/.vue.template"
    },
    "dependencies": {
        "@dsquare-gbu/vue-app": "^1.0.8",
        "@dsquare-gbu/vue-bus": "^1.0.3",
        "@dsquare-gbu/vue-router": "^1.0.7",
        "@dsquare-gbu/vue-services": "^1.0.9",
        "@dsquare-gbu/vue-uses": "^1.0.12",
        "@dsquare-gbu/vue-utils": "^1.1.3",
        "core-js": "^3.6.5",
        "cropperjs": "^1.5.7",
        "vue": "^2.6.11",
        "vue-define": "^1.3.2",
        "vue-head": "^2.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.11.0",
        "@fortawesome/fontawesome-free": "^5.14.0",
        "@node-minify/core": "^6.0.0",
        "@node-minify/cssnano": "^6.0.0",
        "@vue/cli-plugin-babel": "~4.4.0",
        "@vue/cli-plugin-eslint": "~4.4.0",
        "@vue/cli-plugin-router": "~4.4.0",
        "@vue/cli-plugin-vuex": "~4.4.0",
        "@vue/cli-service": "~4.4.0",
        "animate.css": "^4.1.0",
        "babel-eslint": "^10.1.0",
        "blueimp-canvas-to-blob": "^3.28.0",
        "bootstrap": "^4.5.2",
        "cpx": "^1.5.0",
        "dotenv-packed": "^1.2.1",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "flag-icon-css": "^3.5.0",
        "fs-extra": "^9.0.1",
        "html2pug": "^4.0.0",
        "jquery": "^3.5.1",
        "jquery.easing": "^1.4.1",
        "node-sass": "^4.12.0",
        "pug": "^3.0.0",
        "pug-plain-loader": "^1.0.0",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11"
    }
}
