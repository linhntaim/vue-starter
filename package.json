{
    "name": "@linhntaim/vue-starter",
    "version": "1.4.0",
    "private": true,
    "scripts": {
        "init": "npm run @run && npm run @assets",
        "serve": "vue-cli-service serve",
        "build": "npm run init && vue-cli-service build && npm run @build",
        "lint": "npm run init && vue-cli-service lint",
        "@run": "babel ./run/src --out-dir ./run/dist --config-file ./babel.run-config.js --delete-dir-on-start --minified --source-maps",
        "@key": "node ./run/dist/key.js",
        "@assets": "npm run @assets-setup && npm run @assets-compile",
        "@assets-setup": "node ./run/dist/setup.js",
        "@assets-compile": "node ./run/dist/compile.js",
        "@build": "node ./run/dist/build.js",
        "@compose-pug": "node ./run/dist/compose.js --pug ./src/assets/compose/html ./src/assets/compose/pug",
        "@compose-vue": "node ./run/dist/compose.js --vue ./src/assets/compose/html ./src/assets/compose/pug ./src/assets/compose/vue ./src/views/templates/.vue.template"
    },
    "dependencies": {
        "@linhntaim/vue-app": "^1.4.6",
        "@linhntaim/vue-bus": "^1.4.2",
        "@linhntaim/vue-services": "^1.4.5",
        "@linhntaim/vue-uses": "^1.4.10",
        "@linhntaim/vue-utils": "^1.4.6",
        "core-js": "^3.10.0",
        "cropperjs": "^1.5.11",
        "moment": "^2.29.1",
        "vue": "^2.6.12",
        "vue-head": "^2.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/preset-env": "^7.13.12",
        "@node-minify/core": "^6.2.0",
        "@node-minify/cssnano": "^6.2.0",
        "@ttskch/select2-bootstrap4-theme": "^1.5.2",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "animate.css": "^4.1.1",
        "babel-eslint": "^10.1.0",
        "blueimp-canvas-to-blob": "^3.28.0",
        "cpx": "^1.5.0",
        "dotenv-packed": "^1.2.1",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "flag-icon-css": "^3.5.0",
        "fs-extra": "^9.1.0",
        "html2pug": "^4.0.0",
        "node-sass": "^4.12.0",
        "pug": "^3.0.2",
        "pug-plain-loader": "^1.1.0",
        "random-bytes-js": "^1.0.0",
        "sass-loader": "^8.0.2",
        "select2": "^4.0.13",
        "startbootstrap-sb-admin-2": "^4.1.3",
        "tempusdominus-bootstrap-4": "^5.39.0",
        "vue-template-compiler": "^2.6.11",
        "yargs": "^16.2.0"
    }
}
